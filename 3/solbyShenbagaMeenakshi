#include <iostream>
#include <cstdio>
using namespace std;

int main() {
    int a;
    long b;
    char c;
    float d;
    double e;
    printf("Enter the inputs:");
	scanf("%d%li %c%f%lf",&a,&b,&c,&d,&e);//I have just used a space before %c//
	printf("\nOutput:\n");
	printf("%d\n%li\n%c\n%f\n%lf\n",a,b,c,d,e);
    return 0;
}

/*This is because when enter is pressed after giving input for 
  long, that ENTER key is consumed by %c access specifier.*/
